{
  "name": "@devoinc/app-widgets",
  "version": "1.0.6",
  "description": "Devo library that contains the building blocks to start building visual widgets on Devo Apps.",
  "source": "index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "browserslist": [
    "last 4 years",
    "> 1%",
    "not dead",
    "not op_mini all"
  ],
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "locales-copy": "cp -r i18n/locales dist",
    "build": "npm run clean && parcel build --no-cache && npm run locales-copy",
    "type:check": "tsc --noEmit",
    "start": "npm run storybook",
    "storybook": "BABEL_ENV=storybook storybook dev -p 6006",
    "storybook:build": "BABEL_ENV=storybook storybook build -o ./storybook-static-build",
    "docs": "rm -rf docs && typedoc --options typedoc.json ./components/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:DevoInc/App-Widgets.git"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@parcel/babel-plugin-transform-runtime": "^2.4.1",
    "@parcel/babel-preset-env": "^2.4.1",
    "@parcel/packager-ts": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
    "@storybook/addon-actions": "^7.5.2",
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/cli": "^7.5.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.5.2",
    "@storybook/react-webpack5": "^7.5.2",
    "@storybook/testing-library": "^0.0.13",
    "@types/parcel-env": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-loader": "^8.2.5",
    "css-loader": "^5.2.6",
    "eslint": "8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-storybook": "^0.6.1",
    "eslint-plugin-testing-library": "^5.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "parcel": "^2.6.2",
    "prettier": "^2.5.1",

    "sass": "^1.54.0",
    "sass-loader": "^10.1.1",
    "storybook": "^7.5.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.23.15",
    "typescript": "^4.7.4",
    "webpack": "^5.76.0"
  },
  "dependencies": {
    "@nivo/geo": "^0.83.0",
    "@nivo/network": "^0.83.0",
    "@swc/helpers": "^0.4.3",
    "@types/jest": "^29.0.1",
    "@types/uuid": "^8.3.4",
    "apexcharts": "^3.35.5",
